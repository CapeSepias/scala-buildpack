#!/usr/bin/env bash

language='scala'

dependencies=(
  'http://lang-jvm.s3.amazonaws.com/jvm-buildpack-common-v10.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/jdk/cedar-14/openjdk1.9-latest.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/jdk/openjdk1.8.0_20-slim-cedar14-v2.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/jdk/openjdk1.7.0_55-cedar14-v2.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/jdk/openjdk1.6.0_27.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/sbt/v2/sbt-cache-base.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/sbt/v2/sbt-cache-play-2.3_2.10.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/sbt/v2/sbt-cache-play-2.2_2.10.tar.gz'
  'http://lang-jvm.s3.amazonaws.com/sbt/v2/sbt-cache-play-2.1_2.10.tar.gz'
)

excluded_files=(
  '.git/'
  '.gitignore'
  '.gitmodules'
  'cf_spec/'
  'log/'
  'test/'
  'bin/deploy'
  'buildpack-deployer/'
)

BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source $BIN/../buildpack-deployer/lib/deployer
deploy_buildpack $@
